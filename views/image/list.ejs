      <!-- bootbox code -->
      <script src="/lib/bootbox.min.js"></script>
      <script>
          $(document).on("click", ".del_alert", function(e) {
              var imageId = ($(this).attr('id'));
              bootbox.confirm({
                message: "Delete image " + imageId + "? This can be undone!",
                buttons: {
                    confirm: {
                        label: 'Yes',
                        className: 'btn-danger'
                    },
                    cancel: {
                        label: 'No',
                        className: 'btn-success'
                    }
                },
                callback: function (result) {
                    //console.log('This was logged in the callback: ' + result);
                    if(result)
                    {
                      $.post("/eutech/eucaImages/"+imageId + "/delete/", function (data) {
                        location.reload();
                      });
                  }
                }
            });
          });
      </script>

