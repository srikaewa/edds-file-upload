                            <td class="text-center" style="vertical-align:middle;"><%= moment(eucaImages[i].lastedit).format('DD MMM YYYY - HH:mm:ss') %></td>
                            <td class="text-center" style="vertical-align:middle;">
                              <form method="get" action="eucaImages/<%= eucaImages[i].imageId %>/edit">
                                <button type="submit" class="btn btn-default"><i class="fa fa-pencil" aria-hidden="true"></i>&nbsp &nbsp Edit </button>
                              </form>
                            </td>
                            <td  class="text-center" style="vertical-align:middle;">
                              <!--<form method="post" action="eucaImages/<%= eucaImages[i].imageId %>/delete"> -->
                              <!--  <button class="btn btn-danger" data-toggle="confirmation" data-placement="top">Delete</button> -->
                              <!--<form class="form-inline" action="/eutech/eucaImages/<%= eucaImages[i].imageId %>/delete" method="post"> -->
                                  <button class="btn btn-danger <%= eucaImages[i].imageId %>" id="<%= eucaImages[i].imageId %>"><i class="fa fa-minus-circle" aria-hidden="true"></i>&nbsp &nbsp Delete </button>
