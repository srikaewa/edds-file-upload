                                  <script src="/vendors/bootstrap/dist/js/bootstrap.min.js"></script>
                                  <script src="/lib/bootbox.min.js"></script>
                                  <script>
                                      $(document).on("click", ".<%= eucaImages[i].imageId %>", function(e) {
