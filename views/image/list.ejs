                            </td>
                            <td class="text-center" style="vertical-align:middle;">
                              <%= eucaImages[i].level %>
                            </td>
                            <td class="text-center" style="vertical-align:middle;">
                              <%= eucaImages[i].area %>
                            </td>
                            <td class="text-center" style="vertical-align:middle;">
                              <% if(eucaImages[i].validated) { %>
                                <i class="fa fa-check" aria-hidden="true"></i>
                              <% } else { %>
